{
  "name": "Text Drive",
  "description": "Just a text editor.",
  "version": "0.0.19",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "experimental",
    "clipboardRead",
    "clipboardWrite",
    "unlimitedStorage",
    "storage",
    "appWindow",
    "fileSystem"
  ]
}